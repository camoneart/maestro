{
  "worktrees": {
    "path": ".git/orchestra-members",
    "branchPrefix": "feature/"
  },
  "development": {
    "autoSetup": true,
    "defaultEditor": "cursor"
  },
  "postCreate": {
    "copyFiles": [
      ".env"
    ],
    "commands": [
      "pnpm install"
    ]
  },
  "hooks": {
    "beforeDelete": "echo \"演奏者を削除します: $ORCHESTRA_MEMBER\""
  }
}
